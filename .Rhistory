library(stats)
library(tidyverse)
library(TTR)
library(conflicted)
library(DT)
#  Import data sets
file_path <- "C:/Users/USER/Desktop/RSE"
RSE_April_2024_Transactions <- read_excel("C:/Users/USER/Desktop/RSE/RSE April 2024 Transactions.xlsx")
RSE_January_2024_Transactions <- read_excel("C:/Users/USER/Desktop/RSE/RSE January 2024 Transactions.xlsx")
RSE_February_2024_Transactions <- read_excel("C:/Users/USER/Desktop/RSE/RSE February 2024 Transactions.xlsx")
RSE_March_2024_Transactions <- read_excel("C:/Users/USER/Desktop/RSE/RSE March 2024 Transactions.xlsx")
RSE_May_2024_Transactions <- read_excel("C:/Users/USER/Desktop/RSE/RSE May 2024 Transactions.xlsx")
# 1.
# Data cleaning
# Column names of each data set
colnames(RSE_January_2024_Transactions)
colnames(RSE_February_2024_Transactions)
colnames(RSE_March_2024_Transactions)
colnames(RSE_April_2024_Transactions)
colnames(RSE_May_2024_Transactions)
# Renaming columns in each month so as to combine the data sets
RSE_January_2024_Transactions<- RSE_January_2024_Transactions %>% rename("ALL BROKERS TRANSACTIONS FOR MONTH" = "ALL BROKERS TRANSACTIONS FOR JANUARY 2024")
RSE_February_2024_Transactions<- RSE_February_2024_Transactions %>% rename("ALL BROKERS TRANSACTIONS FOR MONTH" = "ALL BROKERS TRANSACTIONS FOR  FEBRUARY 2024")
RSE_March_2024_Transactions<- RSE_March_2024_Transactions %>% rename("ALL BROKERS TRANSACTIONS FOR MONTH" = "ALL BROKERS TRANSACTIONS FOR  MARCH 2024")
RSE_April_2024_Transactions<- RSE_April_2024_Transactions %>% rename("ALL BROKERS TRANSACTIONS FOR MONTH" = "ALL BROKERS TRANSACTIONS FOR APRIL 2024")
RSE_May_2024_Transactions<- RSE_May_2024_Transactions %>% rename("ALL BROKERS TRANSACTIONS FOR MONTH" =  "ALL BROKERS TRANSACTIONS FOR  MAY 2024")
# Remove unnecessary rows
# JANUARY
rows_to_remove <- c(3:5,9:14,17:26,28:30,35:36,41:46,51:52,55:56,58:66,69,77:79,84:94,100:107)
RSE_January_2024_Transactions <- RSE_January_2024_Transactions[-rows_to_remove,]
view(RSE_January_2024_Transactions)
# FEBRUARY
rows_to_remove2 <- c(3:8,12:15,19:22,28:36,40:41,44:45,49:56,62:68,71:74,78:80,84:87,90:96,99:108,112:124)
RSE_February_2024_Transactions <- RSE_February_2024_Transactions[-rows_to_remove2,]
view(RSE_February_2024_Transactions)
# MARCH
rows_to_remove3 <- c(4:15,21:25,34:38,41:45,52:55,57:60,63:75,77,83:84,86:94,97:113,117:121,126:138,142:147)
RSE_March_2024_Transactions <- RSE_March_2024_Transactions[-rows_to_remove3,]
view(RSE_March_2024_Transactions)
# APRIL
rows_to_remove4 <- c(10:11,13,19:22,28:38,45:50,55:60,65:69,75:77,81:88,92:93,95:96,100:102,109:111,118:122,128:130,133:140,142)
RSE_April_2024_Transactions <- RSE_April_2024_Transactions[-rows_to_remove4,]
view(RSE_April_2024_Transactions)
# MAY
rows_to_remove5 <- c(5:9,14:27,33:34,40:43,47,52:54,56:66,76:81,88:91,95:99,101:106,108,111:117,120:124,128:131,138:145,147)
RSE_May_2024_Transactions <- RSE_May_2024_Transactions[-rows_to_remove5,]
view(RSE_April_2024_Transactions)
# Combine cleaned data
combined_RSE_data <- bind_rows(RSE_January_2024_Transactions,RSE_February_2024_Transactions,RSE_March_2024_Transactions,RSE_April_2024_Transactions,RSE_May_2024_Transactions)
rows_to_remove6 <- c(42,43,83,84,129,130,198,199)
combined_RSE_data <- combined_RSE_data[-rows_to_remove6,]
view(combined_RSE_data)
# Check the structure of the data frame
glimpse(combined_RSE_data)
str(combined_RSE_data)
# Check which values are not numeric
non_numeric_values <- combined_RSE_data[!is.na(as.numeric(as.character(combined_RSE_data$...6))) == FALSE, ]
view(combined_RSE_data$...6)
# Remove rows with non-numeric values in the 'value' column
combined_RSE_data <- combined_RSE_data[!is.na(as.numeric(as.character(combined_RSE_data$...6))), ]
combined_RSE_data <- combined_RSE_data[!is.na(as.numeric(as.character(combined_RSE_data$...5))), ]
combined_RSE_data <- combined_RSE_data[!is.na(as.numeric(as.character(combined_RSE_data$...1))), ]
# Ensure the 'value' column is numeric
combined_RSE_data$...6 <- as.numeric(as.character(combined_RSE_data$...6))
view(combined_RSE_data)
# Replace values in the ...1 column from dates format to days from January to May
if (nrow(combined_RSE_data) == 84) {
combined_RSE_data <- combined_RSE_data %>%
mutate(...1 = 1:84)
} else {
stop("The data frame does not have exactly 84 rows.")
}
view(combined_RSE_data)
# Estimation of moving averages
# Set the window size for the moving average
window_size <- 5
# Calculate the moving average
combined_RSE_data$moving_average <- rollmean(combined_RSE_data$...6, k = window_size, fill = NA, align = "right")
# Plot the original data and moving average
plot(combined_RSE_data$...1, combined_RSE_data$...6, type = "l", col = "blue", lwd = 2, ylab = "Price", xlab = "days from jan to may",main = "Moving Averages with original data graph")
# Add a line plot (replace 'density_data' with your actual data)
lines(combined_RSE_data$...1, combined_RSE_data$moving_average, col = "red", lwd = 2)
legend("topright", legend = c("Original", "Moving Averages"), col = c("blue", "red"), lwd = 2)
#  Relative Strength Index
# Calculate RSI
combined_RSE_data$rsi <- RSI(combined_RSE_data$...6, n = 4)
# Print the RSI values
view(combined_RSE_data)
plot(combined_RSE_data$...1, combined_RSE_data$rsi, type = "l", col = "green", lwd = 2, ylab = "rsi", xlab = "days from jan to may",main = "RSI graph")
# Moving Average Covergence Divergence (MACD)
combined_RSE_data$macd <- MACD(x = combined_RSE_data$...6, nFast = 4, nSlow = 8, nSig = 9, maType = "SMA")
View(combined_RSE_data)
plot(combined_RSE_data$macd,type = "h",main = "MACD graph")
# Calculate Bollinger Bands
bb <- BBands(combined_RSE_data$...6, n = 20, sd = 2)
# Combine Bollinger Bands with the original data
combined_RSE_data <- cbind(combined_RSE_data, bb)
view(combined_RSE_data)
# Plot the original data and Bollinger Bands
par(mar = c(4, 4, 4, 5) + 0.1)
plot(combined_RSE_data$...1, combined_RSE_data$...6, type = "l", col = "blue", lwd = 2, ylab = "price", xlab = "Days from jan to may",main = "Bollinger bands graph")
lines(combined_RSE_data$...1, combined_RSE_data$dn, col = "red", lwd = 2)  # Lower Bollinger Band
lines(combined_RSE_data$...1, combined_RSE_data$mavg, col = "green", lwd = 2)  # Middle Bollinger Band
lines(combined_RSE_data$...1, combined_RSE_data$up, col = "black", lwd = 2)  # Upper Bollinger Band
legend("topright", inset = c(-0.20, 0), legend = c("Original", "Lower Band", "Middle Band", "Upper Band"), col = c("blue", "red", "green", "black"), lwd = 2, xpd = TRUE, bty = "n")
combined_RSE_data_estimates <- data.frame(combined_RSE_data$...1,combined_RSE_data$moving_average,combined_RSE_data$rsi,combined_RSE_data$macd,combined_RSE_data$dn,combined_RSE_data$mavg,combined_RSE_data$up,combined_RSE_data$pctB)
view(combined_RSE_data_estimates)
reactable::reactable(combined_RSE_data_estimates)
rmarkdown::render("RSE REPORT")
rmarkdown::render("RSE REPORT.Rmd")
